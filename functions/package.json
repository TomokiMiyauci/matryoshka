    "firebase-functions": "^3.3.0",
    "nuxt": "^2.11.0"
  },
  "devDependencies": {
    "firebase-functions-test": "^0.1.6",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  },
